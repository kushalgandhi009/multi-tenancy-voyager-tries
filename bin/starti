#!/bin/bash
# Logins to the running container.
# Usage: bin/start
cd laradock
cp env-example .env
sed -i "s/PHP_FPM_INSTALL_EXIF=false/PHP_FPM_INSTALL_EXIF=true/g" .env
sed -i "s/NGINX_HOST_HTTP_PORT=80/NGINX_HOST_HTTP_PORT=88/g" .env
sed -i "s/NGINX_HOST_HTTPS_PORT=443/NGINX_HOST_HTTPS_PORT=444/g" .env


docker-compose up -d postgres nginx adminer
cd ..;
